function vgr_mine_heavy_dropped_Open_OnSet(ship)
    startAnim(ship, "Open")
    setPauseTime(ship, "Open", 1000)
end

function vgr_mine_heavy_dropped_Close_OnSet(ship)
    stopAnim(ship, "Open")
    startAnim(ship, "Close")
end
