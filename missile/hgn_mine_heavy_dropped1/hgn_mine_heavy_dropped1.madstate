function HGN_MINE_HEAVY_DROPPED1_Open_OnSet(ship)
    startAnim(ship, "Open")
    setPauseTime(ship, "Open", 1000)
end

function HGN_MINE_HEAVY_DROPPED1_Close_OnSet(ship)
    stopAnim(ship, "Open")
    startAnim(ship, "Close")
end
